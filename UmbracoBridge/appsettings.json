{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UmbracoCMSSettings": {
    "BaseUrl": "<PROTOCOL>://localhost:<PORT>/umbraco/management/api/v1/",
    "GrantType": "client_credentials",
    "ClientId": "<umbraco-back-office-client-id>",
    "ClientSecret": "<umbraco-back-office-client-secret>"
  }
}
